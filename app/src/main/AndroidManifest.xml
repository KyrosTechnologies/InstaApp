<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kyrostechnologies.template.insta">

    <application
        android:name="com.kyrostechnologies.template.insta.data.GlobalVariable"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.kyrostechnologies.template.insta.ActivityMain"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.kyrostechnologies.template.insta.ActivityLogin"
            android:windowSoftInputMode="stateVisible|adjustResize"/>
        <activity android:name="com.kyrostechnologies.template.insta.ActivityHelp">

        </activity>
        <activity android:name=".AboutUs"/>
    </application>

</manifest>